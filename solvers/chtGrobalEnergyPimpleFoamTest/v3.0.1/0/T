FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField      uniform 300;

boundaryField
{

    maxY
    {
        type            fixedValue;
        value           uniform 400;
    }
    minY
    {
        type            fixedValue;
        value           uniform 400;
    }
    minXfirst
    {
        type            empty;
    }
    maxXfirst
    {
        type            empty;
    }
    minZfirst
    {
        type            zeroGradient;
    }
    maxZfirst
    {
        type            zeroGradient;
    }
    minXsecond
    {
        type            empty;
    }
    maxXsecond
    {
        type            empty;
    }
    minZsecond
    {
        type            zeroGradient;
    }
    maxZsecond
    {
        type            zeroGradient;
    }
    minXthird
    {
        type            empty;
    }
    maxXthird
    {
        type            empty;
    }
    minZthird
    {
        type            zeroGradient;
    }
    maxZthird
    {
        type            zeroGradient;
    }
    minXfourth
    {
        type            empty;
    }
    maxXfourth
    {
        type            empty;
    }
    minZfourth
    {
        type            zeroGradient;
    }
    maxZfourth
    {
        type            zeroGradient;
    }
    minXfifth
    {
        type            empty;
    }
    maxXfifth
    {
        type            empty;
    }
    minZfifth
    {
        type            zeroGradient;
    }
    maxZfifth
    {
        type            zeroGradient;
    }
}

