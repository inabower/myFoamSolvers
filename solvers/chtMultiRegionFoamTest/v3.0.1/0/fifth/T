FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fifth";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField      uniform 300;

boundaryField
{

    maxY
    {
        type            fixedValue;
        value           uniform 400;
    }
    minXfifth
    {
        type            empty;
    }
    maxXfifth
    {
        type            empty;
    }
    minZfifth
    {
        type            zeroGradient;
    }
    maxZfifth
    {
        type            zeroGradient;
    }
    fifth_to_fourth
    {
                type            compressible::turbulentTemperatureCoupledBaffleMixed;
                Tnbr            T;
                kappa           solidThermo;
                kappaName       none;
                value           uniform 300;
    }
}

// ************************************************************************* //
